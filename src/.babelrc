{
	"ignore": [
		"node_modules/**"
	],
	"presets": [["@babel/preset-env", { "modules" : false }], "@babel/preset-react"],
	"plugins": [
		["@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true }]
	],
	"env": {
		"test": {
			"presets": ["@babel/preset-env", "@babel/preset-react"],
			"plugins": [
				["@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true }]
			]
		}
	}
}
